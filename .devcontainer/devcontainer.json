{
  "name": "PlutusTx",
  "dockerComposeFile": "docker-compose-plutus.yml",
  "service": "dev",
  "workspaceFolder": "/workspace",
  "shutdownAction": "stopCompose",
  "customizations": {
    "vscode": {
      "extensions": [
        "haskell.haskell",
        "vigoo.stylish-haskell"
      ],
      "settings": {
        "haskell.formattingProvider": "stylish-haskell"
      }
    }
  }
}